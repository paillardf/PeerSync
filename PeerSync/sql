create table events 
(
date numeric,
filepath text,
newhash text,
oldhash text,
action numeric,
parameters text,
owner text,
isFile numeric,
sharedFolder numeric,
status numeric,
PRIMARY KEY (date,filepath,sharedFolder,newhash)
)

create table sharedFolder 
(
uuid text,
rootAbsolutePath text,
peerGroup text,
PRIMARY KEY (uuid)
)



